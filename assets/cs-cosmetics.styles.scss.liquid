    margin-left: 30px;
    li{
      margin-bottom: 0;
    }
  }
  .toolbar-cart {
    &#cart-target{
      position: relative;      
      height: 100%;
      a{
        @extend .dis_table;
      }
    }
    .num-items-in-cart{
      @extend .dis_tablecell;
      .fa{
        display: none;
      }
      span.icon{          
        width: 23px;
        height: 23px; 
        margin-top: -2px;
        background-image: url({{ 'bg-cart.png' | asset_url }}); 
        @include display_inline();
        background-repeat: no-repeat;
        background-position: center center;
        text-align: center;
        color: $color_topheader;

        position: relative;
        .number{
          position: absolute;
          top: 50%;
          margin-top: -3px;
          margin-left: -2.5px;
          font-size: 10px;
          @include display_inline();
          line-height: 15px;
          left: 50%;            
        }
      }
    }
    #cart-info{
      &.dropdown-menu{           
        top: 100%;
        right: 0;
        left: auto;
        border-radius: 0;
        background-color: $bg_cart;
        margin-top: 0;
        padding: 0;
        width: 300px;
        border: 0;
        #cart-content{
          .btn{  
            padding-left: 20px;
            padding-right: 20px;
            font-size: $font_size_12 !important;
            @include btn_dropdowncart($bg_dropdowncart, $text_dropdowncart, $border_dropdowncart);
            &:hover{
              @include btn_dropdowncart_hover($bg_dropdowncart_hover, $text_dropdowncart_hover, $border_dropdowncart_hover);
            }              
          }
          > .empty {
            padding: 20px;
            .btn{
              margin-top: 5px;
              @include display_inline();
            }
          }
          > .items{
            .row{
              @include display_flex(left);
              margin: 0;
              padding: 20px;
              position: relative;
              border-top: 1px solid rgba(255, 255, 255, 0.1);                
            }
            .cart-close{
              position: absolute;
              bottom: 20px;
              right: 20px;
              width: 15px;
              height: 15px;
              background-color: $bg_close;
              border-radius: 100%;
              .fa{
                color: $bg_textclose;
                position: absolute;
                top: 50%;
                left: 50%;                                 
                font-size: 7px;
                margin-top: -3.52px;
                margin-left: -2px;

              }
            }
            .cart-left{
              width: 30%;
            }            
            .cart-right{
              width: 70%;
              padding-left: 15px;
              .cart-title{
                a{
                  letter-spacing: 1px;
                  text-transform: uppercase;
                  font-size:  $font_size_12;
                  color: $color_text_heading;
                }
              }
