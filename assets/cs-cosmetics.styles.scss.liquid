$btn_border: {{settings.border_btn_color}};


$btn_text_hover: {{settings.btn_color_hover}};
$btn_bg_hover: {{settings.bg_btn_color_hover}};
$btn_border_hover: {{settings.border_btn_color_hover}};


@mixin btn($btn_bg, $btn_text, $btn_border ){  
  color: $btn_text;
  background-color: $btn_bg;
  border: 1px solid $btn_border;    
}
@mixin btn_hover($btn_bg_hover, $btn_text_hover, $btn_border_hover){  
  color: $btn_text_hover;
  background-color: $btn_bg_hover;
  border: 1px solid $btn_border_hover;    
}

.btn{
  border-radius: 0 !important;
  text-transform: uppercase;  
  padding: 12px 15px;
  font-size:  $font_size_btn;
  font-weight: $font_weight_bold;
  letter-spacing: 1px;
  font-style: normal;
  @include btn($btn_bg, $btn_text, $btn_border);
  .fa{
    margin-left: 5px;
    color: $btn_text;
  }
  &:focus{
    outline: none;
    @include btn_hover($btn_bg_hover, $btn_text_hover, $btn_border_hover);
    .fa{
      color: $btn_text_hover;
    }
  }
  @media(min-width: 1200px){
    &:hover{
      @include btn_hover($btn_bg_hover, $btn_text_hover, $btn_border_hover);
      .fa{
        color: $btn_text_hover;
      }
    }
  }  
}

