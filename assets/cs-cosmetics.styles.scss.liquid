    /*scroll*/
    /*home_gotodown_layout*/
    $border_scroll: #fbe1de;
    $bg_scroll: #faeeed;
    $color_scroll: #feb0ad;

    $border_scroll_hover: #222222;
    $bg_scroll_hover: #323232;
    $color_scroll_hover: #fff;
    @mixin scroll_item($border_scroll, $bg_scroll, $color_scroll){
      @include display_inline();   
      position: absolute;      
      color: $color_scroll;  
      cursor: pointer;
      width: 70px;
      height: 70px;  
      margin-top: -35px;
      margin-left: -35px;
      &:before{
        content: '';
        position: absolute;
        z-index: 1;
        width: 100%;
        height: 100%;          
        top: 0;
        left: 0;
        border-width: 6px;
        border-style: solid;
        border-color:  $border_scroll;
        background-color: $bg_scroll;
        @include transform_rotate(45deg);
      }
      i{
        color: $color_scroll; 
        position: relative;
        z-index: 2;
        line-height: 70px;        
        font-size: 23px;
        margin-left: 4px;
      }
    }
    .home_gotodown_layout{            
      text-align: center;
      width: 100%;
      float: left;
      clear: both;                          
      z-index: 2;
      position: relative;
      #scroll-to-down{                                           
        @include scroll_item($border_scroll, $bg_scroll, $color_scroll);
      }
    }
    /*End home_gotodown_layout*/
    /*home_gototop_layout*/
    .home_gototop_layout{
      text-align: center;
      width: 100%;
      float: left;
      clear: both;     
      padding: 15px 0;      
      z-index: 2;
      position: relative;
      #scroll-to-top{        
        @include scroll_item($border_scroll_hover, $bg_scroll_hover, $color_scroll_hover);
        margin-top: -49px;
      }
    }
    /*End home_gototop_layout*/
    @media(max-width: 767px){
      .home_gotodown_layout{      
        display: none;
      }
    }
