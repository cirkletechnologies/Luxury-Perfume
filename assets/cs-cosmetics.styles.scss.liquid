          .product-item{
            width: 100%;
            float: left;
            margin-bottom: 30px;
          }
          @media(max-width: 991px){
            .product{
              .row-left{
                width: 40%;
                float: left;
              }
              .row-right{
                width: 60%;
                float: left;
              }
            }
          }
        }
      }
      /*End collection-items*/      
      /*bottom toolbar*/
      .collection-bottom-toolbar{   
        padding: 50px 0 80px;
        width: 100%;
        float: left;
        .pagination_group{
          margin-left: -15px;
          margin-right: -15px;
          .pagination{
            margin: 0;
            float: right;
            @include display_flex(right);                      
            a, span{              
              border-width: 1px;
              border-style: solid;            
              margin-left: 0;
              border-color: $border_pagination;
              color: $text_pagination; 
              background-color: $bg_pagination;            
            }                  
            >li{
              display: block;
              margin-bottom: 0;
              margin-left: 6px;
              &.active{
                a, span{                            
                  border-color: $border_pagination_hover;
                  color: $text_pagination_hover; 
                  background-color: $bg_pagination_hover;
                }
              }
              @media(min-width: 1200px){
                a, span{                            
                  @include transition(all, 500ms, ease);
                }
                &:hover{
                  a, span{                            
                    border-color: $border_pagination_hover;
                    color: $text_pagination_hover; 
                    background-color: $bg_pagination_hover;
                  }
                }
              }           
            }
            >li:first-child,
            >li:last-child{
              a, span{
                border-radius: 0;
              }
            }
            @media(max-width: 767px){
              float: left;
              >li:first-child{
                margin-left: 0;
              }              
            }
          }        
        }
        .product-counter{
          padding-left: 0;
          line-height: 30px;
          letter-spacing: 1px;
          font-size: 14px;
          color:  $color_text_general;
          font-weight: $font_weight_thin;
        }
      }
      /*End bottom toolbar*/      
      /*collection-mainarea*/
      .collection-mainarea{
        padding-left: 0;
        padding-right: 0;
        .collection-items{
          .products{
            @include display_flex(left);
            width: 100%;
            float: left;
          }
          margin-left: -15px;
          margin-right: -15px;          
          &.full_width{
            margin-left: 0px;
            margin-right: 0px;
            .row-right{
              padding: 0 0 0 15px;
            }
            .row-left{
              padding-left: 0;
            }
          }
        }
      }
      /*End collection-mainarea*/
      /*product*/
      .products{
        .product-item{
          @media(max-width: 1199px){
            width: 50%;
          }
          @media(max-width: 991px){
            .row-right{
              right: 0;
              top: 0;
              bottom: auto;
            }
          }    
          @media(max-width: 767px){
            .row-right{
              right: 30px;
              top: 30px;             
            }
          }
          @media(max-width: 600px){
            .row-right{
              right: 15px;
              top: 15px;             
            }
          }
          @media(max-width: 580px){
            .row-right{
              right: 0px;
              top: 0px;             
            }
          }
          @media(max-width: 520px){
            .row-right{
              right: 30px;
              top: 30px;             
            }
          }
        }
      }      
      /*End product*/      
      @media (max-width: 520px){
        .collection-mainarea{
          .product-item{
            width: 100% !important;
          }
        }
      }
    }    
