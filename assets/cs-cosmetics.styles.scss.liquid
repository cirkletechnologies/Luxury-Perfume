  /*NAVIGATION*/
  .navigation_area{      
    float: right;        
    ul.navigation_links{
      margin: 0;
      text-align: center;
      > li{
        @include display_inline();
        text-transform: uppercase;
        padding: 42px 0;
        margin: 0 15px;                 
        > a{
          @include display_inline();
          font-size: $font_size_12;
          color: $color_text_nav;          
          font-weight: $font_weight_normal;
          letter-spacing: 1px;
          position: relative;
          &:focus{
            text-decoration: none;
          }
          &:after{
            position: absolute;
            display: block;
            bottom: 0;
            left: 0;
            content: '';
            width: 100%;
            height: 1px;
            margin-bottom: -2px;
            background-color: $color_text_nav_hover;   
            @include transform_scale(0,1);
          }
          @media(min-width: 1199px){                        
            &:after{              
              @include transition(all, 500ms, ease);          
            }            
          }          
        }
        @media(min-width: 1199px){    
          &:hover{
            > a{
              color: $color_text_nav_hover;
              &:after{
                @include transform_scale(1,1);
              }
            }                       
          }
        }
        &.active{
          > a{
            color: $color_text_nav_hover;
            &:after{
              @include transform_scale(1, 1);
            }            
          }
        }
        &.dropdown{          
          .dropdown-menu{            
            border-radius:0;
            margin-top: 0; 
            padding: 0;
            border: 0;
            .list-title{
              color: $color_text_headingsubnav;
              font-weight: $font_weight_normal;
              font-size: 12px;
            }
            a{                
              color: $color_text_subnav;                
              &:hover{
                color: $color_text_subnav_hover;
              }
              &:focus,
              &:hover{                
                background-color: transparent !important;                                  
              }
            }
          }
          &.mega-menu{   
            position: static;
            .megamenu-container{             
              padding: 40px 50px 40px 20px;              
              background-repeat: no-repeat;
              background-color: #000;
              background-size: cover;
              &.dropdown-menu{
                margin-top: 0 !important;
                border: 0;
              }
              .sub-mega-menu{                   
                list-style: none;
                > li{
                  margin: 0;
                  .col-image{
                    img{
                      @media(min-width: 1200px){
                        width: 100%;
                      }
                    }
                  }
                  .heading{
                    color: $color_text_headingsubnav;
                    font-size:  $font_size_12;
                    position: relative;                  
                    padding-bottom: 12px;
                    margin-bottom: 0;
                    font-weight: $font_weight_bold;
                    font-family: $font_general;
                    letter-spacing: 2px;
                  }
                  .content{
                    color: $color_text_subnav;
                    font-size:  $font_size_12;                                                       
                    text-transform: none;
                    line-height: 2em;
                    letter-spacing: 1px;                      
                    margin-bottom: 13px;
                  }
                  a{
                    color: $color_text_subnav;
                    &:hover{
                      color: $color_text_subnav_hover;
                    }
                  }
                  .linkto{
                    a{
                      font-size: $font_size_12;                      
                      color: $color_text_headingsubnav;
                      text-transform: none;
                      &:hover{
                        color: $color_text_subnav;
                      }
                    }
                  }
                  > ul{      
                    list-style: none;
                    >li{                     
                      margin-bottom: 19px;
                      &.list-title{                     
                        position: relative;
                        padding-bottom: 17px;                      
                        margin-bottom: 16px;
                        &:after{
                          content: '';
                          position: absolute;
                          height: 1px;
                          background-color: $color_text_subnav;
                          bottom: 0;
                          left: 0;
                          width: 37px;
                        }
                      }
                      a{
                        font-size:  $font_size_12;
                        color: $color_text_subnav;
                        font-weight: $font_weight_bold;
                        @media(max-width: 1200px){
                          @include transition(all, 500ms, ease);   
                        }
                        &:hover{
                          color: $color_text_subnav_hover;
                        }
                      }
                      &:last-child{
                        margin-bottom: 0px;
                      }
                    }
                  }
                }
              }                             
              &.megamenu-container-2{                
                @media (min-width: 768px) and (max-width: 991px){
                  .sub-mega-menu{
                    li{
                      &.mega2-collumn1{
                        width: 30%;
                      }
                      &.mega2-column2{
                        width: 35%;
                        > div{
                          width: 100%;
                        }
                        .col-image{
                          margin-bottom: 10px;
                        }
                      }                      
                    }
                  }
                }                
              }
            }
          }
          &:not(.mega-menu){
            .dropdown-menu{                    
              background-color: $bg_subnav;
              font-size:  $font_size_12;  
              min-width: 170px;
              > li{
                border-bottom: $color_text_subnav;
                border-bottom: 1px solid rgba(255, 255, 255, 0.1);                                
                margin-bottom: 0;
                padding: 14px 30px;
                .fa{
                  position: absolute;
                  top: 50%;
                  right: 30px !important;
                  margin-top: -5px;
                }
                a{
                  padding: 0;
                  font-weight: $font_weight_normal;
                }
                &:last-child{
                  border-bottom: 0;
                }
                &.dropdown {
                  &.li-sub-mega{
                    position: relative;
                    .sub-dropdown{                      
                      display: block !important;
                      padding-right: 3px;
                      left: 100%;
                      position: absolute;
                      top: 0;
                      height: 100%;
                    }
                  }
                  > .dropdown-menu{
                    display: block !important;
                    border: 0;
                    left: 100%;
                    top: 0;
                    border: 0;
                    margin-left: 1px;                                        
                  }                  
                  @media(min-width: 1200px){
                    > .dropdown-menu{
                      @include transform_scale(1, 0);
                      @include transition(all, 500ms, ease);   
                    }
                    &:hover{
                      > .dropdown-menu{
                        @include transform_scale(1, 1);                      
                      }
                    }
                  }
                } 
                &.active{
                  a{
                    background-color: transparent;
                    color: $color_text_subnav_hover;
                  }
                }
              }
            }
          }
        } 
        @media(max-width: 1199px){
          margin: 0 12px;                 
        }
      }      
    }
    > ul{
      float: left;
      list-style: none;
    }
    @media(max-width: 1199px){
      .navigation_cart{
        margin-left: 15px;
      }
    }
    @media(max-width: 991px){
      width: 100%;
      float: left;
      position: relative;
      ul.navigation_links{
        text-align: left;
        float: left;
      }
      .navigation_cart{
        float: right;
      }
    }
  }    
  /*END NAVIGATION*/
  /*------------
  END SECTIONS
  -------------*/
