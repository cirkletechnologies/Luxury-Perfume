    $bg_gl_hover: #222;
    $text_gl_hover: #fff;


    @mixin btn_show(){
      height: 40px;
      padding-left: 15px;
      padding-right: 15px;
      border-radius: 0;
      border: 1px solid #f0f1f5;
      background-color: $shop_bg_color;
      margin-left: 15px;    
      span{
        color: $color_text_general;
        font-size: 14px;
      }
      .fa{
        color: $color_text_general;
        margin-left: 5px;
      }
      &:focus{
        outline: none;
      }
    } 
    .collection-desc,
    .collection-banner{
      margin-bottom: 50px;
    }    
    #collection{
      /*collection-toolbar*/  
      .collection-toolbar{
        width: 100%;
        float: left;
        margin-bottom: 50px;      
      }
      .show-per-page{
        float: right;
        position: relative;
        margin-bottom: 5px;
        #showButton{
          @include btn_show();
        }
        #showBox{          
          right: 0;
          left: 0;
          border-radius: 0;
          margin-top: 0;
          padding: 15px;
          border: 0;
        }
      }
      .sortBy{
        float: right;        
        position: relative;
        #sortBox{                    
          right: 0;
          left: auto;
          top: 100%;
          padding: 15px;
          border-radius: 0;
          margin-top: 0;
          border: 0;
        }
        #sortButton{
          @include btn_show();
        }
      }
      .grid_list{
        float: left;
        ul{
          margin-left: 0;
          li{
            height: 40px;
            width: 40px;
            text-align: center;
            margin-bottom: 0;
            margin-right: 10px;
            border-width: 1px;
            border-style: solid;
            border-color: $border_gl;
            background-color: $bg_gl;
            .fa{
              line-height: 40px;
              font-size: 18px;                      
              color: $text_gl;
            }
            &:hover{
              cursor: pointer;
            } 
            &.active{
              border-color: $border_gl_hover;
              background-color: $bg_gl_hover;
              .fa{
                color: $text_gl_hover;
              }
            }
            @media(min-width: 1200px){
              @include transition(all, 300ms, ease);
              &:hover{         
                border-color: $border_gl_hover;
                background-color: $bg_gl_hover;
                .fa{
                  color: $text_gl_hover;
                }
              }
            }                     
          }
        }
      }
      /*TAGE FILTER*/      
      .filter-block{
        width: 100%;
        float: left;
      }      
      #tags-filter-content{
        width: 100%;
        float: left;        
        .filter-tag-group{      
          @include display_flex(left);
          .tag-group{
            padding: 0;   
            width: 100%;           
            float: left;
            .title{
              font-size: $font_size_general;              
              font-family: $font_general;
              border-bottom: 1px solid #ddd;
              border-bottom: 1px solid rgba(0,0,0,0.1);
              font-weight: bold;            
              text-transform: uppercase;          
              height: 35px;
              line-height: 35px;                            
              margin: 10px 0;
              text-align: left;
              @media(max-width: 991px){
                border-left: 0;
              }
              @media(max-width: 360px){
                padding-left: 0px;
              }
            }
            ul{
              -webkit-transition: all 0.3s ease;
              -moz-transition: all 0.3s ease;
              -o-transition: all 0.3s ease;
              -ms-transition: all 0.3s ease;
              transition: all 0.3s ease; 
              margin: 0;
              list-style: none;                
              width: 100%;
              float: left;              
              overflow: auto;              
              padding-right: 15px;
              padding-top: 20px;
              padding-bottom: 20px;             
              @media(max-width: 991px){
                border-left: 0;            
              }
              @media(max-width:360px){                                
                padding-bottom: 10px;
              }
              a{                
                font-size: 14px;
                text-transform: capitalize;
              }
              li.swatch-tag {
                float: left;
                margin-right: 8px;
                margin-bottom: 8px;
                line-height: 20px;
                width: 30px;
                height: 30px;
                borde: 1px solid transparent;
                span{
                  width: 100%;
                  height: 100%;
                  display: block;               
                  background-position: -1px -1px;
                  a{
                    width: 100%;
                    height: 100%;
                    display: block;
                    padding: 0px;                  
                  }
                }
              }
              li.swatch-tag:hover,
              li.swatch-tag.active{
                border: 1px solid #000;
                padding: 2px;            
              }          
              li{
                -webkit-transition: all 0.3s ease;
                -moz-transition: all 0.3s ease;
                -o-transition: all 0.3s ease;
                -ms-transition: all 0.3s ease;
                transition: all 0.3s ease; 
                margin-bottom: 15px;
                a {                                          
                  position: relative;
                  display: block;
                  line-height: 20px;
                  span.fe-checkbox{
                    position: absolute;
                    width: 20px;
                    height: 20px;
                    top: 50%;
                    left: -30px;
                    margin-top: -10px;                
                    background-image: url({{ 'bg_none_check.png' | asset_url }}); 
                    background-repeat: no-repeat;
                    background-position: center center;				
                  }
                }
              }
              li.active{
                a{
                  span.fe-checkbox{                 
                    background-image: url({{ 'bg_check.png' | asset_url }}); 
                  }
                }
              }
              li:not(.swatch-tag){
                width: 100%;
                float: left;
                padding-left: 30px;
                @media(max-width: 1199px){
                  width: 100%;
                }
              }

            }        
          }
          .tag-group:first-child{
            .title{                            
              @media(max-width: 360px){
                padding-left: 0;
              }            
            }          
            ul{
              border-left: 0 !important;
            }
          }      
        }   
        .sidebar-filter-content{
          .filter-tag-group{
            .tag-group{
              width: 100%;
              ul{
                min-height: 0px;                            
                height: auto;
                padding-bottom: 10px;
              }   
              @media(max-width: 360px){
                .title{
                  padding-left: 15px;
                }              
              }
            }
          }
        }
        @media(max-width: 480px){
          margin-bottom: 0px;
        }
      }     
      /*END TAGE FILTER*/ 
      /*End collection-toolbar*/    
      /*collection-items*/
      .collection-items{
        .product{
          margin-bottom: 30px;
        }
        &:not(.full_width){
          .product{           
            @media(min-width: 1200px){
              .row-right{
                @include transition(all, 500ms, ease); 
                @include transform_scale(1, 1);
              }
              &:hover{
                .row-right{
                  @include transform_scale(0, 0);
                }
              }
            }          
          }
        }
        &.full_width{
          width: 100%;
          float: left;
          .product{
            &:hover .row-right{
              @include transform_scale(1,1);
            }
            .row-right{
              position: static;
            }
            .product-group-actions{
              @include display_flex(left);
              .btn{
                padding: 0 25px;
                height: 59px;              
                .fa{
                  line-height: 57px;
                }
              }
              .quickview-wishlist-wrapper {                
                li{
                  height: 59px;
                  width: 59px;
                  > div{
                    height: 100%;
                    > div{
                      height: 100%;
                    }
                  }
