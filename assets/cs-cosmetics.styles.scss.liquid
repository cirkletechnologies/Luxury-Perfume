      right: 0;
      top: 0;
      @include transition_duration(0.5s);  
      @include transform_scale(2, 2); 
      @include transition(all, 500ms, ease);        
      width: 50px;
      z-index: 2;
    }
    &:hover{    
      &:after {      
        @include opacity(1,100);
        @include transform_scale(1, 1);      
      }
      &:before {
        @include opacity(1,100);
        left: 0;
        width: 100%;
      }
    }
  }
