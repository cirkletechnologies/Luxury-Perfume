                width: 100px;
                float: left;
                line-height: 36px;
                @media(max-width: 540px){
                  width: 100%;
                }
              }
              .swatch-element{
                width: 36px;
                height: 36px;
                float: left;
                margin-right: 8px;
                margin-bottom: 10px;
                border-radius: 0%;
                border-width: 1px;
                border-style: solid;
                border-color: transparent;
                transition: all 0.2s ease;
                background-position: -1px -1px;
                position: relative;
                .tooltip{                  
                  position: absolute;
                  bottom: 100%;
                  left: 0;                                  
                  padding: 5px 10px;
                  color: #fff;
                  background-color: #000;
                  border-radius: 3px;
                  margin-bottom: 8px;
                }                
                .tooltip:before,
                .tooltip:after{
                  content: '';
                  position: absolute;
                  left: 30%;
                  bottom: 0;                  
                }
                .tooltip:before{                                                 
                  bottom: -10px;
                  content: " ";
                  display: block;
                  height: 20px;                               
                  width: 100%;
                }
                .tooltip:after{                                    
                  margin-left: 0px;
                  margin-bottom: 0px;
                  bottom: auto;
                  border-left: 4px solid transparent;
                  border-right: 4px solid transparent;
                  border-top: 8px solid #000;
                  top: 100%;
                  margin-top: -2px;
                }
                .crossed-out{
                  display: none;
                }
                input{
                  display: none;
                }
                input:checked + label {
                  box-shadow: none;
                  background-color: #3F3F3F;
                  color: #FFF;
                  border-color: #3F3F3F;
                }
                label{
                  width: 100%;
                  height: 100%;
                  border: 1px solid $border_general;
                  background-color: $border_general;                                    
                  font-size: $font_size_general;
                  font-weight: normal;
                  text-align: center;
                  line-height: 30px;
                  background-position:-1px -1px;
                }
                &.hovered{
                  label{                    
                    background-color: #3F3F3F;
                    color: #FFF;
                    border-color: #3F3F3F;
                  }
                }
              } 
              .swatch-element.default-title{                
                width: auto;
                label{
                  padding-left: 20px;
                  padding-right: 20px;
                }                
              }
              .swatch-element:hover {
                .tooltip{                 
                  opacity: 1;
                  filter: alpha(opacity=100);
                }
              }
              .white{
                label{
                  border: 1px solid $border_general !important;
                }
              }
            }
